<h1>New employe_ccq_trade</h1>

<% form_for(@employe_ccq_trade) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :trade_id %><br />
    <%= f.text_field :tradeid %>
  </p>
  <p>
    <%= f.label :trade_name_fr %><br />
    <%= f.text_field :trade_name_fr %>
  </p>
  <p>
    <%= f.label :trade_name_en %><br />
    <%= f.text_field :trade_name_en %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', employe_ccq_trades_path %>
